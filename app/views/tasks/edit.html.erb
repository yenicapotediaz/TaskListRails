<%= form_for @task do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :completion_status %>
  <%= f.check_box :completion_status %>

  <%= f.label :completion_date %>
  <%= f.text_field :completion_date %>

  <%= f.submit "Submit" %>

<%end%>
